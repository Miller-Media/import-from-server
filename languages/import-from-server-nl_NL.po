#. Description of the plugin
#: import-from-server.php
msgid "Browse files on the server and import them into the WordPress Media Library."
msgstr "Blader door bestanden op de server en importeer ze in de WordPress Mediabibliotheek."

#. Author of the plugin
#: import-from-server.php
msgid "Miller Media"
msgstr "Miller Media"

#. Author URI of the plugin
#: import-from-server.php
msgid "https://www.millermedia.io"
msgstr "https://www.millermedia.io"

